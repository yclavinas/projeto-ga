<?xml version="1.0" encoding="utf-8"?>
<ForecastGroup xmlns="http://www.scec.org/xml-ns/csep/0.1" 
               name="Optimization runs for one-day ETAS model (to generate parameter file for the predictions)">

   <forecastDir>forecasts</forecastDir>   
   <catalogDir>/home/csep/operations/SCEC-natural-laboratory/one-day-models/observations</catalogDir>

   <postProcessing>OneDayModel</postProcessing>
  
   <models>
      ETAS

      <inputs>optimization=True</inputs>
      <schedule>
        <year>
           *
           <month>
              *
              <day>
                 5 10 15 20 25
              </day>  
           </month>
        </year>
     </schedule>
   </models>

</ForecastGroup>
